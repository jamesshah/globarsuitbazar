    <script src="https://js.pusher.com/6.0/pusher.min.js"></script>
  </head>
  <body onload="loaded()">
    <div id="loader">
      <h3>Please wait while we connect you to our customer support...</h3>
    </div>
    <div id="local">
      <div></div>
    </div>
    <div id="container" class="container"></div>
    <div id="callButtons">
      <button class="btn btn-dark" id="mute">Mute</button>
      <button class="btn btn-danger" id="disconnect">End call</button>
    </div>
    <script
      src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
      integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
      crossorigin="anonymous"
